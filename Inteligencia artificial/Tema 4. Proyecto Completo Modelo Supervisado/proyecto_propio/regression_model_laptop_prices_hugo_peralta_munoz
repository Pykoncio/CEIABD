nel.invokeFunction(\n","            'suggestCharts', [key], {});\n","      } catch (error) {\n","        console.error('Error during call to suggestCharts:', error);\n","      }\n","      quickchartButtonEl.classList.remove('colab-df-spinner');\n","      quickchartButtonEl.classList.add('colab-df-quickchart-complete');\n","    }\n","    (() => {\n","      let quickchartButtonEl =\n","        document.querySelector('#df-bc978d5f-2872-40a0-baa9-0db6dfe2c21f button');\n","      quickchartButtonEl.style.display =\n","        google.colab.kernel.accessAllowed ? 'block' : 'none';\n","    })();\n","  </script>\n","</div>\n","\n","    </div>\n","  </div>\n"],"application/vnd.google.colaboratory.intrinsic+json":{"type":"dataframe","variable_name":"laptops_df"}},"metadata":{},"execution_count":610}]},{"cell_type":"code","source":["laptops_df[\"CPU_company\"].unique()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"1C5dQ2CSVOTz","executionInfo":{"status":"ok","timestamp":1731308904982,"user_tz":-60,"elapsed":16,"user":{"displayName":"Hugo Peralta Muñoz","userId":"00397476948593499523"}},"outputId":"106d1f7d-93f7-4694-9050-10fa80820b38"},"execution_count":611,"outputs":[{"output_type":"execute_result","data":{"text/plain":["array(['Intel', 'AMD', 'Samsung'], dtype=object)"]},"metadata":{},"execution_count":611}]},{"cell_type":"code","source":["laptops_df[\"CPU_company\"] = laptops_df[\"CPU_company\"].map({\"Intel\": 0, \"AMD\": 1, \"Samsung\": 2})\n","\n","laptops_df.head()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":382},"id":"WtFNuCXPVX7l","executionInfo":{"status":"ok","timestamp":1731308905248,"user_tz":-60,"elapsed":280,"user":{"displayName":"Hugo Peralta Muñoz","userId":"00397476948593499523"}},"outputId":"0562f66e-3996-4b91-fd99-695cb0e0f099"},"execution_count":612,"outputs":[{"output_type":"execute_result","data":{"text/plain":["  Company      Product  TypeName  Inches  Ram  OS  Weight  Price_euros  \\\n","0   Apple  MacBook P